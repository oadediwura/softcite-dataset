
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@prefix ca: <http://floss.syr.edu/ontologies/2008/4/contentAnalysis.owl#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix vivo: <http://vivoweb.org/ontology/core#> .

@prefix bioj: <http://james.howison.name/ontologies/bio-journal-sample#> .
@prefix citec: <http://james.howison.name/ontologies/software-citation-coding#> .
@prefix bioj-cited: <http://james.howison.name/ontologies/bio-journal-sample-citation#> .
@prefix pmcid: <https://www.ncbi.nlm.nih.gov/pmc/articles/> .
@prefix pmcid-cited: <http://james.howison.name/ontologies/pmcid-journal-sample-citation#> .
@prefix dc: <http://dublincore.org/documents/2012/06/14/dcmi-terms/> .

# https://howisonlab.github.io/softcite-pdf-files/pdf-files/pmc_oa_files/PMC5091050.pdf
pmcid:PMC5091050 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "sahilshahh" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention pmcid:PMC5091050_SS01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
    citec:has_in_text_mention pmcid:PMC5091050_SS02 ;
.
pmcid:PMC5091050_SS01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """And then JER was indexed in the PubMed and PubMed Central.""" ; # use triple quotes

    citec:on_pdf_page 1  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "sahilshahh" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """other""" ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """Looked it up and PubMed is a search engine""" ; # use triple quotes
                         ] ;
        ] ;
.
pmcid:PMC5091050_SS02 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """JER applied to be indexed in the Scopus in March 2014, and it has been under the official evaluation since April 2015.""" ; # use triple quotes

    citec:on_pdf_page 1  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "sahilshahh" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """other""" ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """Looked it up and Scopus is a citation database""" ; # use triple quotes
                         ] ;
        ] ;
.
