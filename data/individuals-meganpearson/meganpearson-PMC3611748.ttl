# https://howisonlab.github.io/softcite-pdf-files/pdf-files/pmc_oa_files/PMC3611748.pdf
pmcid:PMC3611748 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "meganpearson" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention pmcid:PMC3611748_MP01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
    citec:has_in_text_mention pmcid:PMC3611748_MP02 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
    citec:has_in_text_mention pmcid:PMC3611748_MP03 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
.

#itb1
pmcid:PMC3611748_MP01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """Serum glucose levels weremeasured with an autoanalyzer with the manufacturerâ€™scommercially available kits (Cobas 8000, Roche Diagnostics, Mannheim, Germany).""" ; # use triple quotes

    citec:on_pdf_page 1 ; # integer

    citec:spans_pages true ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "meganpearson" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """hardware""" ; # put one in triple quotes
                           ca:certainty 7 ; # integer
                           ca:memo """I looked it up on their website""" ; # use triple quotes
                         ] ;
        ] ;
.

