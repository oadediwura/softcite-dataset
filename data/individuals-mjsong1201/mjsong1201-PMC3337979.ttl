.
pmcid:PMC3337979_MS01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """Data were analysed using the GraphPad Prism 4.""" ; # use triple quotes

    citec:on_pdf_page 8  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label  """software"""  ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """GraphPad Prism 4 is a program developed by GraphPad Software.""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
