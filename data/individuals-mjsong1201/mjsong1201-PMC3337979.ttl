pmcid:PMC3337979 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;

    
    citec:has_in_text_mention pmcid:PMC3337979_MS01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
